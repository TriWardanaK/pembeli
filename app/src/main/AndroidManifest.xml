<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pembeli">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pembeli">
        <activity android:name=".barang.PesananBarangActivity"></activity>
        <activity android:name=".barang.WishlistBarangActivity" />
        <activity
            android:name=".DetailActivity"
            android:theme="@style/Theme.Pembeli.NoActionBar" />
        <activity android:name=".view.pembeli.UpdateActivity" />
        <activity android:name=".view.pembeli.SignupActivity" />
        <activity android:name=".view.pembeli.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>